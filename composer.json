{
    "name": "berriart/sitemap-bundle",
    "description": "This bundle provides a way to create an xml sitemap using doctrine",
    "keywords": ["sitemap","command","seo"],
    "type": "symfony-bundle",
    "license": "GPL-2.0",
    "authors": [
        {
            "name": "Alberto Varela",
            "email": "alberto@berriart.com"
        }
    ],
    "require": {
        "php": ">=5.3.9",
        "symfony/framework-bundle": ">=2.3|~3.0",
        "doctrine/common": ">=2.0",
        "doctrine/doctrine-bundle": "*"
    },
    "autoload": {
        "psr-0": { "Berriart\\Bundle\\SitemapBundle": "" }
    },
    "target-dir": "Berriart/Bundle/SitemapBundle",
    "support": {
        "docs": "https://github.com/artberri/BerriartSitemapBundle/blob/master/Resources/doc/index.md"
    }
}
